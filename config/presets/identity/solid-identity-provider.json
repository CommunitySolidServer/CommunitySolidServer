{
  "@context": "https://linkedsoftwaredependencies.org/bundles/npm/@solid/community-server/^0.0.0/components/context.jsonld",
  "@graph": [
    {
      "@id": "urn:solid-server:default:IdpStorage",
      "@type": "JsonResourceStorage",
      "JsonResourceStorage:_source": {
        "@id": "urn:solid-server:default:ResourceStore"
      }
    },
    {
      "@id": "urn:solid-server:default:IdpStorageAdapterFactory",
      "@type": "ClientWebIdFetchingStorageAdapterFactory",
      "ClientWebIdFetchingStorageAdapterFactory:_wrappedAdapterFactory": {
        "@type": "ResourceStoreStorageAdapterFactory",
        "ResourceStoreStorageAdapterFactory:_args_baseUrl": {
          "@id": "urn:solid-server:default:variable:baseUrl"
        },
        "ResourceStoreStorageAdapterFactory:_args_storagePathname": "/idp/oidc",
        "ResourceStoreStorageAdapterFactory:_args_store": {
          "@type": "PassthroughKeyValueStore",
          "PassthroughKeyValueStore:_store": {
            "@id": "urn:solid-server:default:ResourceStore"
          }
        }
      }
    },
    {
      "@id": "urn:solid-server:default:IdpConfigurationGenerator",
      "@type": "KeyGeneratingIdpConfigurationGenerator",
      "KeyGeneratingIdpConfigurationGenerator:_storageAdapterFactory": {
        "@id": "urn:solid-server:default:IdpStorageAdapterFactory"
      },
      "KeyGeneratingIdpConfigurationGenerator:_baseUrl": {
        "@id": "urn:solid-server:default:variable:baseUrl"
      },
      "KeyGeneratingIdpConfigurationGenerator:_storage": {
        "@id": "urn:solid-server:default:IdpStorage"
      }
    },
    {
      "@id": "urn:solid-server:default:IdentityProviderFactory",
      "@type": "IdentityProviderFactory",
      "IdentityProviderFactory:_issuer": {
        "@id": "urn:solid-server:default:variable:baseUrl"
      },
      "IdentityProviderFactory:_configurationFactory": {
        "@id": "urn:solid-server:default:IdpConfigurationGenerator"
      }
    },
    {
      "@id": "urn:solid-server:default:IdentityProviderHttpHandler",
      "@type": "IdentityProviderHttpHandler",
      "IdentityProviderHttpHandler:_providerFactory": {
        "@id": "urn:solid-server:default:IdentityProviderFactory"
      },
      "IdentityProviderHttpHandler:_interactionPolicy": {
        "@id": "urn:solid-server:email-password-interaction:EmailPasswordInteractionPolicy"
      },
      "IdentityProviderHttpHandler:_interactionHttpHandler": {
        "@id": "urn:solid-server:email-password-interaction:InteractionHttpHandler"
      },
      "IdentityProviderHttpHandler:_errorResponseWriter": {
        "@type": "ErrorResponseWriter"
      }
    },
    {
      "@id": "urn:solid-server:default:IdentityProviderHandler",
      "@type": "RouterHandler",
      "RouterHandler:_allowedMethods": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "RouterHandler:_allowedPathNames": [ "^/idp/.*", "^/\\.well-known/openid-configuration" ],
      "RouterHandler:_handler": { "@id": "urn:solid-server:default:IdentityProviderHttpHandler" }
    }
  ]
}
